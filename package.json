{
  "name": "ts-image-viewer",
  "version": "0.3.7",
  "description": "browser image viewer with basic painting support",
  "module": "dist/ts-image-viewer.esm.js",
  "types": "dist/ts-image-viewer.d.ts",
  "scripts": {
    "test": "karma start",
    "copycsstotsc": "copyfiles -f ./src/*.css ./tsc/src",
    "copypngtotsc": "copyfiles -f ./src/assets/icons/*.png ./tsc/src/assets/icons && copyfiles -f ./src/assets/assets.d.ts ./tsc/src",
    "buildts": "tsc && npm run copypngtotsc",
    "buildru": "rollup -c",
    "build": "npm run buildts && npm run buildru",
    "start": "npm run build && lite-server -c ls-config.json",
    "mpublish": "npm version minor && npm publish",
    "ppublish": "npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yermolim/ts-image-viewer.git"
  },
  "keywords": [
    "web",
    "typescript",
    "canvas",
    "image",
    "viewer",
    "image-viewer",
    "png",
    "jpg",
    "webp",
    "drawing"
  ],
  "author": "Volodymyr Yermolenko <yermolim@gmail.com> (https://github.com/yermolim)",
  "license": "AGPL",
  "bugs": {
    "url": "https://github.com/yermolim/ts-image-viewer/issues"
  },
  "homepage": "https://github.com/yermolim/ts-image-viewer#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-multi-entry": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-strip": "^2.1.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jasmine": "^3.9.1",
    "@types/resize-observer-browser": "^0.1.6",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "jasmine-core": "^3.9.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-typescript": "^5.5.2",
    "lite-server": "^2.6.1",
    "puppeteer": "^10.2.0",
    "rollup": "^2.56.3",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-license": "^2.5.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "mathador": "^0.2.2",
    "ts-viewers-core": "^0.0.20",
    "uuid": "^8.3.2"
  }
}
